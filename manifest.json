{
  "name": "Feedblocker",
  "description": "Remove feeds from LinkedIn.",
  "version": "0.2",
  "minimum_chrome_version": "16.0.884",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["feeds.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Remove LinkedIn feed"
  },
  "manifest_version": 2,
  "icons": { "128": "icon128.png" }
}
